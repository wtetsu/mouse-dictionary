{
  "name": "mouse-dictionary",
  "version": "1.3.9",
  "repository": "https://github.com/wtetsu/mouse-dictionary.git",
  "author": "wtetsu",
  "license": "MIT",
  "scripts": {
    "build": "npm run build-chrome && npm run build-firefox",
    "watch": "webpack -w --config webpack.config.chrome.js",
    "release": "npm run release-chrome && npm run release-firefox",
    "lint": "eslint src/**/*.{js,jsx} && prettier --check src/**/*.{js,jsx}",
    "format": "prettier --write src/**/*.{js,jsx}",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-cov": "npm test -- --coverage",
    "build-chrome": "webpack --config webpack.config.chrome.js",
    "build-chrome-beta": "webpack --config webpack.config.chrome.beta.js",
    "watch-chrome": "webpack -w --config webpack.config.chrome.js",
    "release-chrome": "NODE_ENV=production npm run build-chrome",
    "release-chrome-beta": "NODE_ENV=production npm run build-chrome-beta",
    "build-firefox": "webpack --config webpack.config.firefox.js",
    "watch-firefox": "webpack -w --config webpack.config.firefox.js",
    "release-firefox": "NODE_ENV=production npm run build-firefox"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "deinja": "0.0.3",
    "hogan.js": "^3.0.2",
    "immer": "^6.0.2",
    "react": "^16.13.1",
    "react-ace": "^8.1.0",
    "react-color": "^2.18.0",
    "react-contenteditable": "^3.3.3",
    "react-dom": "^16.13.1",
    "sprintf-js": "^1.1.2",
    "sweetalert": "^2.1.2",
    "uniqlist": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "milligram": "^1.3.0",
    "prettier": "^2.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
